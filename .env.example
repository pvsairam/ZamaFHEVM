# Database
DATABASE_URL=postgresql://user:password@host:port/database

# WalletConnect (for RainbowKit)
# Get your project ID from: https://cloud.walletconnect.com/
VITE_WALLETCONNECT_PROJECT_ID=your_project_id_here

# FHE Keys (will be generated automatically)
FHE_PRIVATE_KEY_PATH=./secrets/fhe-private-key.pem
FHE_PUBLIC_KEY_PATH=./secrets/fhe-public-key.pem

# Sepolia RPC (for on-chain anchoring)
SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/YOUR_INFURA_KEY
SEPOLIA_PRIVATE_KEY=your_deployer_private_key_here

# Session Secret
SESSION_SECRET=your_secure_random_session_secret_here
